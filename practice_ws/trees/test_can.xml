<root>
    <BehaviorTree ID="test_can">
    
    <Sequence name="process_can">
        <SetDetector name="set_can_dt" func_name="color_dtcs.d_coke" id="None"/>
        <Watch name="confirm_can"/>
        <GetObjFront name="calc_approach_pos"/> 
        <GoToPose name="move_to_front"/>
        <ReachObj name="reach_can"/> 
        <Pick name="prepare_grip"/>
        <ObjectLocation name="can_location"/>
        <Shift name="shift" target="[0.175]"/>
        <Sleep name="sleep" time="[3]"/>
        <Fit2 name="fine_tuning" distance="[0.175]"/> 
        <Close name="grasp_can"/> 
        <ArmHome name="lift_can"/>
        
        <SetDestination name="set_safe_zone" value="[(-0.105, -1.05, -0.00143)]"/>
        <GoToPose name="move_to_safe"/>
        
        <Open name="release_can"/> 
        <Shift name="back_off" target="[-0.2]"/>
        <SetDestination name="set_start_pos" value="[(0.0, 0.0, 0.0)]"/>
        <GoToPose name="return_start"/>
    </Sequence>

    </BehaviorTree>
</root>